##by NOPEname

execute if score $in nn.math.trig matches 0..6 run function nope_name.math:zz_private/trig/sin_lt/0
execute if score $in nn.math.trig matches 7..12 run function nope_name.math:zz_private/trig/sin_lt/7
execute if score $in nn.math.trig matches 13..18 run function nope_name.math:zz_private/trig/sin_lt/13
execute if score $in nn.math.trig matches 19..24 run function nope_name.math:zz_private/trig/sin_lt/19
execute if score $in nn.math.trig matches 25..30 run function nope_name.math:zz_private/trig/sin_lt/25
