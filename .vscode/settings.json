{
	"editor.insertSpaces": false, // TABS!!!
	"files.eol": "\n",
	"datapack.env.cmdVersion": "1.20",
	"datapack.env.dataVersion": "1.20-pre1",
	"datapack.env.defaultVisibility": "public",
	"datapack.env.dependsOnVanilla": true,
	"datapack.env.detectionDepth": 1,
	"datapack.env.permissionLevel": 2,
	"datapack.lint.blockStateBracketSpacing": { "inside": 0 },
	"datapack.lint.blockStateCommaSpacing": { "before": 0, "after": 1 },
	"datapack.lint.blockStateEqualSpacing": { "before": 0, "after": 1 },
	"datapack.lint.blockStateSortKeys": null,
	"datapack.lint.blockStateTrailingComma": false,
	"datapack.lint.eol": "LF",
	"datapack.lint.idOmitDefaultNamespace": ["warning", false],
	"datapack.lint.nameOfNbtCompoundTagKeys": null, //["warning", "camelCase"],
	"datapack.lint.nameOfObjectives": [
		"warning",
		[
			{
				"label": "fetchr.snake_case",
				"parts": [ "/^fetchr$/", "snake_case" ],
				"sep": "."
			},
			{
				"label": "author.name_space.snake_case",
				"parts": [ "snake_case", "snake_case", "snake_case" ],
				"sep": "."
			}
		]
	],
	"datapack.lint.nameOfScoreHolders": [
		"warning",
		[
			{
				"label": "$snake_case",
				"prefix": "$",
				"parts": "snake_case"
			},
			{
				"label": "$semi/path/to_function.snake_case_name",
				"prefix": "$",
				"parts": [
					{
						"parts": "snake_case",
						"sep": "/",
						"allowMoreParts": true
					},
					"snake_case"
				],
				"sep": ".",
			},
			{
				"label": "numeric constants",
				"parts": "/^(?:0|-?[1-9][0-9]*)$/"
			}
		]
	],
	"datapack.lint.nameOfTags": [
		"warning",
		[
			{
				"label": "fetchr.snake_case",
				"parts": [ "/fetchr/", "snake_case" ],
				"sep": "."
			},
			{
				"label": "author.name_space.snake_case",
				"parts": [ "snake_case", "snake_case", "snake_case" ],
				"sep": "."
			}
		]
	],
	"datapack.lint.nameOfTeams": [
		"warning",
		{
			"label": "namespace.snake_case",
			"parts": [ "snake_case", "snake_case" ],
			"sep": "."
		}
	],
	"datapack.lint.nbtArrayBracketSpacing": { "inside": 0 },
	"datapack.lint.nbtArrayCommaSpacing": { "before": 0, "after": 1 },
	"datapack.lint.nbtArrayLengthCheck": ["error", true],
	"datapack.lint.nbtArraySemicolonSpacing": { "after": 1 },
	"datapack.lint.nbtArrayTrailingComma": false,
	"datapack.lint.nbtBoolean": ["warning", true],
	"datapack.lint.nbtByteSuffix": "b",
	"datapack.lint.nbtCompoundBracketSpacing": { "inside": 0 },
	"datapack.lint.nbtCompoundColonSpacing": { "before": 0, "after": 1 },
	"datapack.lint.nbtCompoundCommaSpacing": { "before": 0, "after": 1 },
	"datapack.lint.nbtCompoundKeyQuote": null, //["warning", false],
	"datapack.lint.nbtCompoundKeyQuoteType": ["warning", "prefer double"],
	"datapack.lint.nbtCompoundSortKeys": null,
	"datapack.lint.nbtCompoundTrailingComma": false,
	"datapack.lint.nbtDoubleOmitSuffix": false,
	"datapack.lint.nbtDoubleSuffix": "d",
	"datapack.lint.nbtFloatSuffix": "f",
	"datapack.lint.nbtListBracketSpacing": { "inside": 0 },
	"datapack.lint.nbtListCommaSpacing": { "before": 0, "after": 1 },
	"datapack.lint.nbtListLengthCheck": [ "error", true ],
	"datapack.lint.nbtListTrailingComma": false,
	"datapack.lint.nbtLongSuffix": "l",
	"datapack.lint.nbtPathQuote": null,//["warning", false],
	"datapack.lint.nbtPathQuoteType": ["error", "always double"], // single quotes should never be used here, see https://bugs.mojang.com/browse/MC-175504
	"datapack.lint.nbtShortSuffix": "s",
	"datapack.lint.nbtStringQuote": ["warning", true],
	"datapack.lint.nbtStringQuoteType": ["warning", "prefer double"],
	"datapack.lint.nbtTypeCheck": ["warning", "strictly"],
	"datapack.lint.selectorBracketSpacing": { "inside": 0 },
	"datapack.lint.selectorCommaSpacing": { "before": 0, "after": 1 },
	"datapack.lint.selectorEqualSpacing": { "before": 0, "after": 1 },
	"datapack.lint.selectorKeyQuote": ["warning", false],
	"datapack.lint.selectorKeyQuoteType": ["warning", "always double"],
	"datapack.lint.selectorSortKeys": null,
	"datapack.lint.selectorTrailingComma": false,
	"datapack.lint.strictAdvancementCheck": ["error", true],
	"datapack.lint.strictAttributeCheck": ["error", "always"],
	"datapack.lint.strictBlockCheck": ["error", "always"],
	"datapack.lint.strictBlockTagCheck": ["error", true],
	"datapack.lint.strictBossbarCheck": ["error", true],
	"datapack.lint.strictDimensionTypeCheck": ["error", "always"],
	"datapack.lint.strictEnchantmentCheck": ["error", "always"],
	"datapack.lint.strictEntityTypeCheck": ["error", "always"],
	"datapack.lint.strictEntityTypeTagCheck": ["error", true],
	"datapack.lint.strictFluidCheck": ["error", "always"],
	"datapack.lint.strictFluidTagCheck": ["error", true],
	"datapack.lint.strictFunctionCheck": ["error", true],
	"datapack.lint.strictFunctionTagCheck": ["error", true],
	"datapack.lint.strictItemCheck": ["error", "always"],
	"datapack.lint.strictItemTagCheck": ["error", true],
	"datapack.lint.strictLootTableCheck": ["error", true],
	"datapack.lint.strictMobEffectCheck": ["error", "always"],
	"datapack.lint.strictMotiveCheck": ["error", "always"],
	"datapack.lint.strictObjectiveCheck": ["error", true],
	"datapack.lint.strictParticleTypeCheck": ["error", "always"],
	"datapack.lint.strictPotionCheck": ["error", "always"],
	"datapack.lint.strictPredicateCheck": ["error", true],
	"datapack.lint.strictRecipeCheck": ["error", true],
	"datapack.lint.strictSoundEventCheck": ["error", "always"],
	"datapack.lint.strictScoreHolderCheck": ["error", true],
	"datapack.lint.strictStorageCheck": ["error", true],
	"datapack.lint.strictTagCheck": ["error", true],
	"datapack.lint.strictTeamCheck": ["error", true],
	"datapack.lint.stringQuote": ["warning", false],
	"datapack.lint.stringQuoteType": ["warning", "prefer double"],
	"datapack.lint.timeOmitTickUnit": true
}